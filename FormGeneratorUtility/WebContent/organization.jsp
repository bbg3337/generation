<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html ><html><head><title>organization</title><link rel="stylesheet" href="../css/bootstrap.min.css"><link rel="stylesheet" href="../css/jquery.datetimepicker.min.css"><script src="../js/jquery-3.2.1.min.js"></script><script src="../js/bootstrap.min.js"></script><script src="../js/jquery.datetimepicker.full.js"></script><script src="../js/formutility.js"></script><script src="../js/custom_alert.js"></script><script src="../js/bootstrap-show-password.min.js"></script></head><body><div class="container"><center><h2>organization</h2></center><form action="../insertForm/organization" class="form-horizontal" method="post" onsubmit="return checkFormData()"><div class="form-group"><label class="control-label col-sm-4">organization name</label><div class="col-sm-5"><input type="text"  class="form-control" name="organization_name" id="organization_name" tabindex="1" pattern="^[A-Za-z0-9]{1}[A-Za-z0-9\s]{0,}$" title="Only alphanumeric allow." required maxlength="12" /></div></div><div class="form-group"><label class="control-label col-sm-4">organization desc</label><div class="col-sm-5"><textarea name="organizationdesc" id="organizationdesc"  class="form-control" placeholder="Enter text..." rows="5" cols="30" tabindex="2" required pattern="[A-Z]{0,}" maxlength="15"  ></textarea></div></div><div class="form-group"><label class="control-label col-sm-4">organization area</label><div class="col-sm-5"> <div class='row'> <div class='col-sm-6'><input type="radio" name="organization_area" id="organization_area1" tabindex="3" required pattern=":pattern"  value=Ambavadi /> Ambavadi<br /></div> <div class='col-sm-6'><input type="radio" name="organization_area" id="organization_area2" tabindex="3" required pattern=":pattern"  value=Panchvati /> Panchvati<br /></div> <div class='col-sm-6'><input type="radio" name="organization_area" id="organization_area3" tabindex="3" required pattern=":pattern"  value=Nikol /> Nikol<br /></div></div></div></div><div class="form-group"><label class="control-label col-sm-4">organization speciality</label><div class="col-sm-5"><select class="form-control" name="organization_speciality" id="organization_speciality" tabindex="6" required pattern=":pattern"  /><option value='IT'>IT</option><option value='HRMS'>HRMS</option><option value='CA'>CA</option></select></div></div><div class="form-group"><label class="control-label col-sm-4">organization date</label><div class="col-sm-5"><input type="text"  class="form-control" name="organizationdate__" id="organizationdate__" tabindex="10" required onkeypress="return false;"  /></div></div><div class="form-group"><label class="control-label col-sm-4">organization link</label><div class="col-sm-5"><a href='organization$Link%' tabindex=11 >organization link</a></div></div><div class="form-group"><div class="col-sm-offset-4 col-sm-5"><input type="submit" class="btn btn-success" /></div></div></form><font color='red'><p id="errMessage"></p></font><c:if test="${not empty message}"><font color='green'><c:out value="${message }"></c:out></font></c:if><c:if test="${not empty errMessage}"><font color='red'> ${errMessage } </font></c:if></div><div class="container" style="overflow-x:auto;"><c:if test="${not empty listData}" >	<table class="table"> 	<thead style='color:#ccffe6;background-color:#87ceeb'>		<tr><th>organization name</th><th>organization desc</th><th>organization area</th><th>organization speciality</th><th>organization date</th>	</tr></thead>	<c:forEach items="${listData}" var="obj">	<tbody><tr><td class="col-m-2"><c:out value="${obj.organization_name}" /></td><td class="col-m-2"><c:out value="${obj.organizationdesc}" /></td><td class="col-m-2"><c:out value="${obj.organization_area}" /></td><td class="col-m-2"><c:out value="${obj.organization_speciality}" /></td><td class="col-m-2"><c:out value="${obj.organizationdate__}" /></td>	</tr></tbody>	</c:forEach></table></c:if></div></body><script src="../js/formutility_date.js"></script><script>function checkFormData() {return true; }</script><script>$('#organizationdate__').datetimepicker({timepicker:false,maxDate:'+1970/01/01',format: 'Y-m-d',formatDate : 'Y-m-d',});</script></html>
