<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head><title>Emp</title><link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"><script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script><script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script></head><body><div class="container"><center><h2>Emp</h2></center><form action="../insertForm/Emp" class="form-horizontal" method="post"><div class="form-group"><label class="control-label col-sm-4">Employee ID</label><div class="col-sm-5"><input type="text"  class="form-control" name="EMPID" id="EMPID" tabindex="1"  /></div></div><div class="form-group"><label class="control-label col-sm-4">Employee Name</label><div class="col-sm-5"><textarea name="empName" id="empName"  class="form-control" placeholder="Enter text..." rows="5" cols="30" tabindex="2"  ></textarea></div></div><div class="form-group"><label class="control-label col-sm-4">Employee Address</label><div class="col-sm-5"><textarea name="empAdd" id="empAdd"  class="form-control" placeholder="Enter text..." rows="5" cols="30" tabindex="3"  ></textarea></div></div><div class="form-group"><div class="col-sm-offset-4 col-sm-5"><input type="submit" class="btn btn-success" /></div></div></form><c:if test="${not empty message}"><c:out value="${message }"></c:out></c:if></div><div class="container" style="overflow-x:auto;"><c:if test="${not empty listData}" ><table class="table"> 	<thead style='color:#ccffe6;background-color:#87ceeb'>		<tr><th>Employee ID</th><th>Employee Name</th><th>Employee Address</th></tr></thead><c:forEach items="${listData}" var="obj"><tbody><tr><td class="col-m-2"><c:out value="${obj.empid}" /></td><td class="col-m-2"><c:out value="${obj.empname}" /></td><td class="col-m-2"><c:out value="${obj.empadd}" /></td></tr></tbody></c:forEach></c:if></table></body></html>
