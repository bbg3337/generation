<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head><title>country</title><link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"><script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script><script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script></head><body><div class="container"><center><h2>country</h2></center><form action="../insertForm/country" class="form-horizontal" method="post"><div class="form-group"><label class="control-label col-sm-4">Country code</label><div class="col-sm-5"><input type="text"  class="form-control" name="cnt_code" id="cnt_code" tabindex="1"  /></div></div><div class="form-group"><label class="control-label col-sm-4">ABC2</label><div class="col-sm-5"><input type="text"  class="form-control" name="abc2" id="abc2" tabindex="11"  /></div></div><div class="form-group"><label class="control-label col-sm-4">Country Name</label><div class="col-sm-5"><input type="text"  class="form-control" name="cnt_name" id="cnt_name" tabindex="2"  /></div></div><div class="form-group"><label class="control-label col-sm-4">State code</label><div class="col-sm-5"><input type="text"  class="form-control" name="state_code" id="state_code" tabindex="3"  /></div></div><div class="form-group"><label class="control-label col-sm-4">State name</label><div class="col-sm-5"><input type="text"  class="form-control" name="state_name" id="state_name" tabindex="4"  /></div></div><div class="form-group"><label class="control-label col-sm-4">City code</label><div class="col-sm-5"><input type="text"  class="form-control" name="city_code" id="city_code" tabindex="5"  /></div></div><div class="form-group"><label class="control-label col-sm-4">temp13</label><div class="col-sm-5"><input type="text"  class="form-control" name="temp13" id="temp13" tabindex="55"  /></div></div><div class="form-group"><label class="control-label col-sm-4">City name</label><div class="col-sm-5"><input type="text"  class="form-control" name="city_name" id="city_name" tabindex="6"  /></div></div><div class="form-group"><div class="col-sm-offset-4 col-sm-5"><input type="submit" class="btn btn-success" /></div></div></form><c:if test="${not empty message}"><c:out value="${message }"></c:out></c:if></div><div class="container" style="overflow-x:auto;"><c:if test="${not empty listData}" ><table class="table"> 	<thead style='color:#ccffe6;background-color:#87ceeb'>		<tr><th>Country code</th><th>ABC2</th><th>Country Name</th><th>State code</th><th>State name</th><th>City code</th><th>temp13</th><th>City name</th></tr></thead><c:forEach items="${listData}" var="obj"><tbody><tr><td class="col-m-2"><c:out value="${obj.cnt_code}" /></td><td class="col-m-2"><c:out value="${obj.abc2}" /></td><td class="col-m-2"><c:out value="${obj.cnt_name}" /></td><td class="col-m-2"><c:out value="${obj.state_code}" /></td><td class="col-m-2"><c:out value="${obj.state_name}" /></td><td class="col-m-2"><c:out value="${obj.city_code}" /></td><td class="col-m-2"><c:out value="${obj.temp13}" /></td><td class="col-m-2"><c:out value="${obj.city_name}" /></td></tr></tbody></c:forEach></c:if></table></body></html>
