<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><!DOCTYPE html ><html><head><title>GovtEmployee</title><link rel="stylesheet" href="../css/bootstrap.min.css"><link rel="stylesheet" href="../css/jquery.datetimepicker.min.css"><link rel="stylesheet" href="../css/forms.css"><script src="../js/jquery-3.2.1.min.js"></script><script src="../js/bootstrap.min.js"></script><script src="../js/jquery.datetimepicker.full.js"></script><script src="../js/formutility.js"></script><script src="../js/custom_alert.js"></script><script src="../js/bootstrap-show-password.min.js"></script><script src="../js/js-form-validator.min.js"></script></head><body><a href='../index' class='glyphicon glyphicon-home btn-lg' style='margin: 10px 20px; color: black;'></a><div class="container"><center><h2>GovtEmployee</h2></center><form action="../insertForm/GovtEmployee" id="idGovtEmployee" class="form-horizontal" method="post" ><div class="form-group"><label class="control-label col-sm-4">Name</label><div class="col-sm-5"><input type="text"  class="form-control" name="emp_nm" id="emp_nm" tabindex="1" maxlength="50" data-rule="required|Only_aplha" /></div></div><div class="form-group"><label class="control-label col-sm-4">Address</label><div class="col-sm-5"><textarea name="emp_addr" id="emp_addr"  class="form-control" placeholder="Enter text..." rows="5" cols="30" tabindex="2" maxlength="100"  data-rule="required|Aplha_numeric" ></textarea></div></div><div class="form-group"><label class="control-label col-sm-4">Employment Type</label><div class="col-sm-5"> <div class='row'> <div class='col-sm-6'><input type="radio" name="emp_type" id="emp_type" tabindex="3"  value=On Payroll data-rule="required" />On Payroll<br /></div> <div class='col-sm-6'><input type="radio" name="emp_type" id="emp_type" tabindex="3"  value=On Contract data-rule="required" />On Contract<br /></div></div></div></div><div class="form-group"><label class="control-label col-sm-4">Hobbby</label><div class="col-sm-5"> <div class='row'> <div class='col-sm-6'><input type="checkbox" name="emp_hobby" id="emp_hobby1" tabindex="4"  value=Reading /> Reading<br /></div> <div class='col-sm-6'><input type="checkbox" name="emp_hobby" id="emp_hobby2" tabindex="4"  value=Music /> Music<br /></div> <div class='col-sm-6'><input type="checkbox" name="emp_hobby" id="emp_hobby3" tabindex="4"  value=Cricket /> Cricket<br /></div></div></div></div><div class="form-group"><label class="control-label col-sm-4">Marrital Status</label><div class="col-sm-5"><select class="form-control" name="emp_mar_status" id="emp_mar_status" tabindex="5"   /><option value='Single'>Single</option><option value='Married'>Married</option><option value='Divorcee'>Divorcee</option><option value='Widow'>Widow</option></select></div></div><div class="form-group"><label class="control-label col-sm-4">Birthdate</label><div class="col-sm-5"><input type="text"  class="form-control" name="emp_dob" id="emp_dob" tabindex="6" onkeypress="return false;" readonly  data-rule="required" /></div></div><div class="form-group"><label class="control-label col-sm-4">Experience</label><div class="col-sm-5"><input type="number"  class="form-control" name="emp_exp" id="emp_exp" tabindex="7" step="1"  data-rule="required|Digit_Only" /></div></div><div class="form-group"><label class="control-label col-sm-4">Company website</label><div class="col-sm-5"><a href='http://www.silvertouch.com/' tabindex=9 >Company website</a></div></div><div class="form-group"><label class="control-label col-sm-4">Password</label><div class="col-sm-5"><input type="password"  class="form-control" name="emp_pwd" id="emp_pwd" tabindex="10" maxlength="15"  data-rule="required|Password" /><input type="checkbox" onclick="showPassword('emp_pwd')" >Show Password</div></div><div class="form-group"><label class="control-label col-sm-4">info</label><div class="col-sm-5"> <div class='row'> <div class='col-sm-6'><input type="checkbox" name="info" id="info1" tabindex="11"  value=a /> a<br /></div> <div class='col-sm-6'><input type="checkbox" name="info" id="info2" tabindex="11"  value=b /> b<br /></div> <div class='col-sm-6'><input type="checkbox" name="info" id="info3" tabindex="11"  value=c /> c<br /></div> <div class='col-sm-6'><input type="checkbox" name="info" id="info4" tabindex="11"  value=d /> d<br /></div></div></div></div><div class="form-group"><div class="col-sm-offset-4 col-sm-5"><input type="submit" class="btn btn-success" id="submitGovtEmployee" value="Submit" />&nbsp <button class='btn btn-default' data-toggle='modal' data-target='#myModal' tabindex=8 onclick="alertFuntion('Age Message','your age must be above 18.','TYPE.20','20','TYPE.25','25','TYPE.30','30') ">Age Message</button></div></div></form><font color='red'><p id="errMessage"></p></font><c:if test="${not empty message}"><font color='green'><c:out value="${message }"></c:out></font></c:if><c:if test="${not empty errMessage}"><font color='red'> ${errMessage } </font></c:if></div><div class="container" style="overflow-x:auto;"><c:if test="${not empty listData}" >	<table class="table"> 	<thead style='color:#ccffe6;background-color:#87ceeb'>		<tr><th>Name</th><th>Address</th><th>Employment Type</th><th>Hobbby</th><th>Marrital Status</th><th>Birthdate</th><th>Experience</th><th>Password</th><th>info</th>	</tr></thead>	<c:forEach items="${listData}" var="obj">	<tbody><tr><td class="col-m-2"><c:out value="${obj.emp_nm}" /></td><td class="col-m-2"><c:out value="${obj.emp_addr}" /></td><td class="col-m-2"><c:out value="${obj.emp_type}" /></td><td class="col-m-2"><c:out value="${obj.emp_hobby}" /></td><td class="col-m-2"><c:out value="${obj.emp_mar_status}" /></td><td class="col-m-2"><c:out value="${obj.emp_dob}" /></td><td class="col-m-2"><c:out value="${obj.emp_exp}" /></td><td class="col-m-2"><c:out value="${obj.emp_pwd}" /></td><td class="col-m-2"><c:out value="${obj.info}" /></td>	</tr></tbody>	</c:forEach></table></c:if><jsp:include page='alertForm.jsp'></jsp:include></div></body><script src="../js/formutility_date.js"></script><script>$(function(){ $('#submitGovtEmployee').click(function() {if(!$('input[name=emp_hobby]:checked').length > 0) {document.getElementById("errMessage").innerHTML = "Hobbby can not be null";return false;}if(!$('input[name=info]:checked').length > 0) {document.getElementById("errMessage").innerHTML = "info can not be null";return false;}document.getElementById("errMessage").innerHTML = ""; return true; }); });</script><script>new Validator(document.querySelector('#idGovtEmployee'), function (err, res) {return res;},options = { rules : { Only_aplha: function(a){ return (new RegExp('^[A-Za-z]{0,}$')).test(a) },Aplha_numeric: function(a){ return (new RegExp('^[A-Za-z0-9]{1}[A-Za-z0-9\\s]{0,}$')).test(a) },Digit_Only: function(a){ return (new RegExp('^[1-9]{1,}[0-9]{0,}$')).test(a) },Password: function(a){ return (new RegExp('(?=.*[0-9])(?=.*[a-z])(?=.*[A-Z].*[A-Z])(?=.*[@#$%^&+=].*[@#$%^&+=])(?=\\S+$).{8,}')).test(a) },},messages : { en : { Only_aplha:{ empty: "Only alphabets allow.", incorrect: "Only alphabets allow." },Aplha_numeric:{ empty: "Only alphanumeric allow.", incorrect: "Only alphanumeric allow." },Digit_Only:{ empty: "Only digit allow, first character must be not Zero.", incorrect: "Only digit allow, first character must be not Zero." },Password:{ empty: "Password must be, 2 Latter Capital,2 special char ,and any later and any digit", incorrect: "Password must be, 2 Latter Capital,2 special char ,and any later and any digit" },}}});</script><script>$('#emp_dob').datetimepicker({timepicker:false,maxDate:'+1970/01/01',format: 'd.m.Y',formatDate : 'd.m.Y',});</script></html>
